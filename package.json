    "express": "^4.14.0",
    "mongoose": "^4.7.2",
    "morgan": "^1.7.0",
    "nodemon": "^1.11.0",
    "request": "^2.79.0",
    "require-all": "^2.1.0",
    "rxjs": "^5.0.1",
